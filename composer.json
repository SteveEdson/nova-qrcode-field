{
    "name": "steveedson/nova-qrcode-field",
    "description": "Nova QR code field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kristories\\Qrcode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kristories\\Qrcode\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
